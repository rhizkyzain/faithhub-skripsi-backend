{
    "version": 2,
    "builds": [
    {
    "src": "./index.js",
    "use": "@vercel/node"
    }
    ],
    "routes": [
        {
          "src": "/api/auth/(.*)",
          "dest": "/bin/app/routes/user.js",
          "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
          "src": "/api/question/(.*)",
          "dest": "/bin/app/routes/question.js",
          "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
          "src": "/api/article/(.*)",
          "dest": "/bin/app/routes/article.js",
          "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
        }
      ]
    
    
 }