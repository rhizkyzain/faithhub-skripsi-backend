{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(login|signup|signupAdmin|myProfile)",
        "methods": ["POST", "GET"],
        "dest": "/bin/app/routes/user.js"
      },
      {
        "src": "/api/question/(create|update|delete|reply|vote|fetchAll|getTags|getAllTags|uploadAudio|getAudioContent|getQuestionbyTags|searchContent|getUserQuestion|getDetail|sortReplies)/:questionId*",
        "methods": ["POST", "PUT", "DELETE", "GET"],
        "dest": "/bin/app/routes/question.js"
      },
      {
        "src": "/api/article/(createArticle|get/all|get/:articleId|getArticlebyTags|reply/:articleId|vote/:articleId)",
        "methods": ["POST", "GET"],
        "dest": "/bin/app/routes/article.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }